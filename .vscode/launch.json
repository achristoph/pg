{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "CodeLens (Launch) - List",
      "request": "launch",
      "mainClass": "collection.list.List",
      "projectName": "pg_2e44ffdc"
    },
    {
      "type": "java",
      "name": "CodeLens (Launch) - list",
      "request": "launch",
      "mainClass": "collection.list.list",
      "projectName": "pg_2e44ffdc"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "JS Debug",
      "program": "${file}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "TS Debug",
      "program": "${file}",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "name": "Python: Terminal (integrated)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Launch current file w/ ts-node",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "args": [
        "${file}"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "console": "integratedTerminal"
    }
  ]
}
